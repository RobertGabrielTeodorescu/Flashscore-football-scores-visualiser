<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminSummaryController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="player1TextField" alignment="CENTER" layoutX="126.0" layoutY="85.0" promptText="Player1" />
            <TextField fx:id="player2TextField" alignment="CENTER" layoutX="126.0" layoutY="110.0" promptText="Player2" />
            <TextField fx:id="minuteTextField" alignment="CENTER" layoutX="126.0" layoutY="135.0" promptText="Minute" />
            <TextField fx:id="typeTextField" alignment="CENTER" layoutX="126.0" layoutY="160.0" promptText="Type" />
            <Button layoutX="154.0" layoutY="247.0" mnemonicParsing="false" onAction="#addSummaryButton" text="Add Summary" />
            <TextField fx:id="penaltyTextField" alignment="CENTER" layoutX="126.0" layoutY="185.0" promptText="Penlaty" />
         </children>
    </AnchorPane>
  </children>
</VBox>
